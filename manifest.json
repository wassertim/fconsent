{
    "name": "FConsent",
    "description": "FConsent is a simple extension that allows you to easily automate your consent to multiple websites.",        
    "version": "1.0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://*/*",
                "http://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "*.js"
            ],
            "matches": ["<all_urls>"]
        }
    ]
}